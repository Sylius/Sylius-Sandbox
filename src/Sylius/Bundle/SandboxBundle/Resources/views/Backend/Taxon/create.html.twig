{% extends 'SyliusSandboxBundle:Backend:layout.html.twig' %}

{% block header %}
    <h1>Creating taxon</h1>
{% endblock %}

{% block content %}
<form action="{{ path('sylius_sandbox_backend_taxon_create') }}" method="post" class="form-horizontal" novalidate>
    {% include 'SyliusSandboxBundle:Backend/Taxon:_form.html.twig' %}
    <div class="form-actions">
       <input type="submit" value="create taxon" class="btn btn-large btn-primary" />
       &nbsp;
       <input type="reset" value="cancel" class="btn btn-large" />
    </div>
</form>
{% endblock %}
